.head
	position: absolute 0 0 *
	z-index: 3
	padding: 45px 0 0
	color: #fff

	&__wrap
		display: flex
		align-items: center
		justify-content: space-between

.logo
	display: block

	img
		display: inherit

.h-btns
	display: flex

	&__btn

		&:not(:last-child)
			margin-right: 31px

.h-menu
	display: flex

	li

		&:not(:last-child)
			margin-right: 40px

	&__link
		font-weight: 500
		font-size: em(18)
		color: inherit